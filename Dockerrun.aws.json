{
    "AWSEBDockerrunVersion":3,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "neogeowild/multi-client",
            "hostname":"client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "neogeowild/multi-server",
            "hostname":"api",
            "essential": false,
            "memory": 128
        },
        {
            "name":"worker",
            "image":"neogeowild/multi-worker",
            "hostname":"worker",
            "essential": false,
            "memory": 128
        },
        {
            "name":"nginx",
            "image":"neogeowild/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "postMappings":[
                {
                    "containerPort": 80,
                    "hostPort": 80                    
                }
            ],
            "links": ["client", "server"],
            "memory": 128
        }
    ]
}